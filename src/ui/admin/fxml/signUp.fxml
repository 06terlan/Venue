<GridPane alignment="CENTER" hgap="1.0" maxHeight="-Infinity" vgap="1.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.admin.controller.SignUpController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
   <children>
      <Label text="First Name" GridPane.rowIndex="1" />
      <Label text="Last Name" GridPane.rowIndex="2" />
      <Label text="Phone No." GridPane.rowIndex="3" />
      <Label text="Address Street" GridPane.rowIndex="4" />
      <Label text="City" GridPane.rowIndex="5" />
      <Label text="State" GridPane.rowIndex="6" />
      <Label text="Zip" GridPane.rowIndex="7" />
      <Label text="User Name" GridPane.rowIndex="8" />
      <Label text="Password" GridPane.rowIndex="9" />
      
      <TextField fx:id="firstName" prefHeight="25.0" prefWidth="118.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="lastName" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="phoneNo" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextArea fx:id="street" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <TextField fx:id="city" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <TextField fx:id="state" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <TextField fx:id="zip" GridPane.columnIndex="1" GridPane.rowIndex="7" />
